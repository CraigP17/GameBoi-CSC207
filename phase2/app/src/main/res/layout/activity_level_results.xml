<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ScorePages.LevelResults">

    <Button
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/bangers"
        android:onClick="toNext"
        android:text="@string/toRock"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


    <TextView
        android:id="@+id/textView16"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="324dp"
        android:fontFamily="@font/luckiest_guy"
        android:text="@string/flashLives"
        android:textAlignment="textEnd"
        android:textColor="@android:color/holo_red_light"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lives"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="300dp"
        android:fontFamily="@font/bangers"
        android:textColor="@android:color/holo_red_light"
        android:textSize="40sp"
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="224dp"
        android:layout_height="75dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/luckiest_guy"
        android:text="@string/oldMultiplier"
        android:textAlignment="textEnd"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/currMultiply"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/bangers"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="40sp"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/lives" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/luckiest_guy"
        android:text="@string/flashTotal"
        android:textAlignment="textEnd"
        android:textColor="@android:color/holo_purple"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/currScore"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/bangers"
        android:textColor="@android:color/holo_orange_light"
        android:textSize="40sp"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/currMultiply" />

    <TextView
        android:id="@+id/goTextNext"
        android:layout_width="377dp"
        android:layout_height="79dp"
        android:layout_marginBottom="64dp"
        android:fontFamily="@font/bangers"
        android:text="@string/flashWin2"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.636"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/minus1"
        android:layout_width="222dp"
        android:layout_height="293dp"
        android:fontFamily="@font/bangers"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="200sp"
        app:layout_constraintBottom_toTopOf="@+id/winLoss"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/winLoss"
        android:layout_width="313dp"
        android:layout_height="118dp"
        android:layout_marginTop="184dp"
        android:fontFamily="@font/bangers"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/winText"
        android:layout_width="329dp"
        android:layout_height="213dp"
        android:layout_marginTop="88dp"
        android:fontFamily="@font/bangers"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="55sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>