<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FlashWin">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="410dp"
        android:layout_height="120dp"
        android:layout_marginTop="52dp"
        android:fontFamily="@font/bangers"
        android:text="@string/flashWin1"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/bangers"
        android:onClick="toNext"
        android:text="@string/toRock"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="367dp"
        android:layout_height="124dp"
        android:layout_marginBottom="64dp"
        android:fontFamily="@font/bangers"
        android:text="@string/flashWin2"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.636"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="184dp"
        android:fontFamily="@font/luckiest_guy"
        android:text="@string/flashLives"
        android:textAlignment="textEnd"
        android:textColor="@android:color/holo_red_light"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="200dp"
        android:layout_height="55dp"
        android:layout_marginStart="76dp"
        android:fontFamily="@font/luckiest_guy"
        android:text="@string/flashTotal"
        android:textAlignment="textEnd"
        android:textColor="@android:color/holo_purple"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView32" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/bangers"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/bangers"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/textView30"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/bangers"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/textView32"
        app:layout_constraintTop_toBottomOf="@+id/textView31" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="225dp"
        android:layout_height="55dp"
        android:layout_marginStart="52dp"
        android:fontFamily="@font/luckiest_guy"
        android:text="@string/RPSScore"
        android:textAlignment="textEnd"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView30" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/bangers"
        android:textColor="@android:color/holo_orange_light"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView33" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="225dp"
        android:layout_height="55dp"
        android:layout_marginStart="52dp"
        android:fontFamily="@font/luckiest_guy"
        android:text="@string/flashLvlScore"
        android:textAlignment="textEnd"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="172dp"
        android:fontFamily="@font/bangers"
        android:textColor="@android:color/holo_red_light"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="225dp"
        android:layout_height="55dp"
        android:layout_marginStart="52dp"
        android:fontFamily="@font/luckiest_guy"
        android:text="@string/mathScore"
        android:textAlignment="textEnd"
        android:textColor="@android:color/holo_blue_dark"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

</androidx.constraintlayout.widget.ConstraintLayout>